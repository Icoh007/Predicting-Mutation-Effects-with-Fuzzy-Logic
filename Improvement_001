        "# @title Instalando e importando as bibliotecas\n",
        "!pip install scikit-fuzzy\n",
        "\n",
        "import numpy as np\n",
        "import skfuzzy as fuzzy\n",
        "from skfuzzy import control as ctrl"
      ]
